  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jason-live/wci-cli.git"
  },
  "author": "jasonlive",
  "license": "MIT",
  "bin": {
    "wci": "./bin/wci"
  },
  "bugs": {
